double multiply(double mid,int n)
{
    double ans=1.0;
    for(int i=0;i<n;i++)
    {
        ans=ans*mid;
    }
    return ans;
}

double findNthRootOfM(int n, long long m) {
	// Write your code here.
    double low=0;
    double high=m;
    double limit=1e-6;
    double mid;
    
    while((high-low)>limit)
    {
        mid=(high+low)/2.0;
        if(multiply(mid,n)>m)
            high=mid;
        else
            low=mid;
    }
    return mid;
}
